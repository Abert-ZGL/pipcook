{
  "name": "@pipcook/daemon",
  "version": "2.0.0",
  "description": "pipcook daemon",
  "keywords": [
    "pipcook",
    "machine-learning",
    "loopback-application",
    "loopback"
  ],
  "main": "dist",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=12.19"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "yarn run prettier:check && yarn run eslint",
    "lint:fix": "yarn run eslint:fix && yarn run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "yarn run prettier:cli -l",
    "prettier:fix": "yarn run prettier:cli --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "yarn run eslint --fix",
    "pretest": "yarn run rebuild",
    "test": "ava",
    "cov": "nyc --reporter=lcov ava",
    "cov:report": "nyc report --reporter=html",
    "posttest": "yarn run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && yarn run posttest",
    "premigrate": "npm run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "npm run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "rebuild": "yarn run clean && yarn run build",
    "prestart": "yarn run rebuild",
    "start": "node -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache"
  },
  "repository": {
    "type": "git"
  },
  "author": "anonymous",
  "license": "Apache 2.0",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^3.1.0",
    "@loopback/core": "^2.12.0",
    "@loopback/repository": "^3.2.0",
    "@loopback/rest": "^9.0.0",
    "@loopback/rest-explorer": "^3.0.3",
    "@loopback/service-proxy": "^3.0.3",
    "@pipcook/costa": "^1.2.0",
    "@pipcook/pipcook-core": "^1.2.0",
    "debug": "^4.3.1",
    "loopback-connector-sqlite3": "git+ssh://git@github.com:FeelyChau/loopback-connector-sqlite3.git",
    "multer": "^1.4.2",
    "queue": "^6.0.1",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "ssestream": "^1.1.0",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^6.2.6",
    "@loopback/eslint-config": "^10.0.2",
    "@loopback/testlab": "^3.2.8",
    "@types/multer": "^1.4.5",
    "@types/node": "^10.17.44",
    "@types/request-promise": "^4.1.46",
    "ava": "^3.13.0",
    "eslint": "^7.12.1",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.19",
    "typescript": "~4.0.5"
  }
}
